<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:asigbe="http://schemas.android.com/apk/res/com.asigbe.slidekeyboardpro"
	android:layout_width="fill_parent" android:layout_height="wrap_content"
	android:orientation="vertical">
	<TextView android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:layout_gravity="center_horizontal"
		android:text="@string/get_dictionaries" android:layout_marginBottom="20dip" />
	<Button android:id="@+id/nextButton"
		android:layout_width="200dip" android:layout_height="wrap_content"
		android:text="@string/next" android:layout_gravity="center_horizontal" />
</LinearLayout>